float value; 
float amp;

void setup(){
  Serial.begin(9600);
  pinMode(A0, INPUT);
}

void loop(){
  value = analogRead(A0);
  amp = (((value - 511)*5/1024)/0.185)*1000;
  Serial.print("Raw = ");
  Serial.print(value);
  Serial.print("    Calc = ");
  Serial.print(amp);
  Serial.println(" mA");
  delay(100);
}
